{
  {% for post in site.posts %}

    "{{ post.url | slugify }}": {
      "title": "{{ post.title | xml_escape }}",
      "url": " {{ post.url | xml_escape }}",
      "category": "{{ post.categories }}",
	  "content": "{{ post.content }}"
    }
    {% unless forloop.last %},{% endunless %}
  {% endfor %}
}